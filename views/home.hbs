<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Action</th>


    </tr>
  </thead>
  <tbody>
    {{#each rows}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td>
        <a href="#" type="button" class="btn btn-light btn-small"> View</a>
        <a href="#" type="button" class="btn btn-light btn-small"> Edit</a>
        <a href="#" type="button" class="btn btn-light btn-small"> Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>